<voodoo>
   <!-- This script tests all "simple" HTML events, which is most of them -->

   <browser url="{@global.url}/tests/test-data/simple.html"/>

   <!-- DIV -->
   <div id="div" assert="div text"/>
   <div id="div" assertnot="foobar"/>
   <div id="div" click="true"/>
   <div id="div" assert="clicked"/>
   <div id="div" jscriptevent="onmousedown"/>
   <div id="div" assert="mousedown"/>
   <div id="div" save="mydiv"/>
   <div id="div" var="mydiv"/>
   <!-- This div is not visible, but required is true - shouldn't be an error-->
   <div id="InvisibleDiv" click="true" required="false"/>

   <!-- SPAN -->
   <span id="span" assert="span text"/>
   <span id="span" assertnot="foobar"/>
   <span id="span" click="true"/>
   <span id="span" assert="clicked"/>
   <span id="span" jscriptevent="onmousedown"/>
   <span id="span" assert="mousedown"/>
   <span id="span" save="myspan"/>
   <span id="span" var="myspan"/>

   <!-- H1 through H6 -->
   <h1 id="h1" click="true"/>
   <div id="click_test" assert="H1"/>
   <h2 id="h2" click="true"/>
   <div id="click_test" assert="H2"/>
   <h3 id="h3" click="true"/>
   <div id="click_test" assert="H3"/>
   <h4 id="h4" click="true"/>
   <div id="click_test" assert="H4"/>
   <h5 id="h5" click="true"/>
   <div id="click_test" assert="H5"/>
   <h6 id="h6" click="true"/>
   <div id="click_test" assert="H6"/>

</voodoo>
